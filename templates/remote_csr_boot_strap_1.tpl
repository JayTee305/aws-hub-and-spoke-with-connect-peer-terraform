ios-config-1="hostname ${hostname}" 
ios-config-2="no ip domain lookup" 
ios-config-3="interface GigabitEthernet2" 
ios-config-4="ip address dhcp" 
ios-config-5="no shut" 
ios-config-6="crypto isakmp policy 101"
ios-config-7="encryption aes 128"
ios-config-8="authentication pre-share"
ios-config-9="group 2"
ios-config-10="lifetime 28800"
ios-config-11="hash sha"
ios-config-12="crypto isakmp key ${isakmp_secret} address ${remote_peer_tunnel_ip}"
ios-config-13="crypto isakmp invalid-spi-recovery"
ios-config-14="crypto isakmp keepalive 10 10"
ios-config-15="crypto ipsec security-association replay window-size 1024"
ios-config-16="crypto ipsec transform-set ESP_AES128_SHA esp-aes esp-sha-hmac "
ios-config-17="mode tunnel"
ios-config-18="crypto ipsec profile IPSec_Profile"
ios-config-19="set security-association lifetime kilobytes disable"
ios-config-20="set transform-set ESP_AES128_SHA  "
ios-config-21="set pfs group2"
ios-config-22="interface Tunnel${tunnel_number}"
ios-config-23="ip address ${tunnel_source_ip} ${tunnel_source_mask}"
ios-config-24="keepalive 10 3"
ios-config-25="tunnel source GigabitEthernet1"
ios-config-26="tunnel mode ipsec ipv4"
ios-config-27="tunnel destination ${remote_peer_tunnel_ip}"
ios-config-28="tunnel protection ipsec profile IPSec_Profile"
ios-config-29="no shut" 
ios-config-30="router bgp ${local_bgp_asn}"
ios-config-31="bgp log-neighbor-changes"
ios-config-32="neighbor ${tunnel_destination_ip} remote-as ${remote_tunnel_bgp_asn}"
ios-config-33="neighbor ${tunnel_destination_ip} ebgp-multihop 2"
ios-config-34="neighbor ${tunnel_destination_ip} timers 10 30 30"
ios-config-35="address-family ipv4"
ios-config-36="network ${network_cidr} mask ${network_mask}"
ios-config-37="neighbor ${tunnel_destination_ip} activate"
ios-config-38="neighbor ${tunnel_destination_ip} soft-reconfiguration inbound"
ios-config-39="ip route 10.251.0.0 255.255.0.0 null0 200"