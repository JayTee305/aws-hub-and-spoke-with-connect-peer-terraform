ios-config-1="hostname ${hostname}" 
ios-config-2="no ip domain lookup" 
ios-config-3="interface Tunnel ${cgw_tunnel_interface} 
ios-config-4="tunnel mode gre ip" 
ios-config-5="ip address ${cgw_tunnel_ip_address} 255.255.255.248" 
ios-config-6="tunnel source ${cgw_tunnel_source}" 
ios-config-7="tunnel destination ${cgw_tunnel_destination}" 
ios-config-08="interface GigabitEthernet2" 
ios-config-09="ip address dhcp" 
ios-config-10="no shut" 
ios-config-11="crypto isakmp policy 101"
ios-config-12="encryption aes 128"
ios-config-13="authentication pre-share"
ios-config-14="group 2"
ios-config-15="lifetime 28800"
ios-config-16="hash sha"
ios-config-17="crypto isakmp key ${isakmp_secret} address ${remote_peer_tunnel_ip}"
ios-config-18="crypto isakmp invalid-spi-recovery"
ios-config-19="crypto isakmp keepalive 10 10"
ios-config-20="crypto ipsec security-association replay window-size 1024"
ios-config-21="crypto ipsec transform-set ESP_AES128_SHA esp-aes esp-sha-hmac "
ios-config-22="mode tunnel"
ios-config-23="crypto ipsec profile IPSec_Profile"
ios-config-24="set security-association lifetime kilobytes disable"
ios-config-25="set transform-set ESP_AES128_SHA  "
ios-config-26="set pfs group2"
ios-config-27="interface Tunnel${tunnel_number}"
ios-config-28="ip address ${tunnel_source_ip} ${tunnel_source_mask}"
ios-config-29="keepalive 10 3"
ios-config-30="tunnel source GigabitEthernet1"
ios-config-31="tunnel mode ipsec ipv4"
ios-config-32="tunnel destination ${remote_peer_tunnel_ip}"
ios-config-33="tunnel protection ipsec profile IPSec_Profile"
ios-config-34="no shut" 
ios-config-35="ip route ${tunnel_cidr_block} ${tunnnel_cidr_mask} ${vpc_router}" 
ios-config-36="ip route 172.20.0.0 255.255.0.0 null 0 200" 
ios-config-37="ip route 172.21.0.0 255.255.0.0 null 0 200" 
ios-config-38="ip route 172.22.0.0 255.255.0.0 null 0 200" 
ios-config-39="ip route 172.23.0.0 255.255.0.0 null 0 200" 
ios-config-40="router bgp ${cgw_asn}" 
ios-config-41="bgp log-neighbor-changes" 
ios-config-42="network ${network_cidr} mask ${network_mask}" 
ios-config-43="neighbor ${aws_tunnel_ip_1} remote-as ${aws_asn}" 
ios-config-44="neighbor ${aws_tunnel_ip_1} ebgp-multihop 2" 
ios-config-45="neighbor ${aws_tunnel_ip_1} timers 10 30 30" 
ios-config-46="neighbor ${aws_tunnel_ip_2} remote-as ${aws_asn}" 
ios-config-47="neighbor ${aws_tunnel_ip_2} ebgp-multihop 2" 
ios-config-48="neighbor ${aws_tunnel_ip_2} timers 10 30 30" 
ios-config-49="neighbor ${tunnel_destination_ip} remote-as ${remote_tunnel_bgp_asn}"
ios-config-50="neighbor ${tunnel_destination_ip} ebgp-multihop 2"
ios-config-51="neighbor ${tunnel_destination_ip} timers 10 30 30"
ios-config-52="address-family ipv4" 
ios-config-53="network 172.20.0.0 mask 255.255.0.0" 
ios-config-54="network 172.21.0.0 mask 255.255.0.0" 
ios-config-55="network 172.22.0.0 mask 255.255.0.0" 
ios-config-56="network 172.23.0.0 mask 255.255.0.0" 
ios-config-57="neighbor ${aws_tunnel_ip_1} activate" 
ios-config-58="neighbor ${aws_tunnel_ip_1} soft-reconfiguration inbound" 
ios-config-59="neighbor ${aws_tunnel_ip_2} activate" 
ios-config-60="neighbor ${aws_tunnel_ip_2} soft-reconfiguration inbound" 
ios-config-61="neighbor ${tunnel_destination_ip} activate"
